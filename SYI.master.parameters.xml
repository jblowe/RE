<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet href="../../styles/params2html.xsl" type="text/xsl"?>
<params>
  <param name="title" value="Southern Yi, mostly based on Hansson, 1989"/>
  <param name="correspondences" value="SYI.TOC.u8.csv"/>

  <attested name="Akha" file="Akha.data.xml"/>
  <attested name="HaniL" file="HaniL.data.xml"/>
  <attested name="HaniW" file="HaniW.data.xml"/>
  <attested name="Haoni" file="Haoni.data.xml"/>
  <attested name="Khatu" file="Khatu.data.xml"/>
  <attested name="Mpi" file="Mpi.data.xml"/>
  <attested name="PL" file="PL.data.xml"/>
  <attested name="Pijo" file="Pijo.data.xml"/>
  <attested name="WB" file="WB.data.xml"/>

  <csvdata name="data" file="SYI.TAB.u8.csv"/>
  <mel name="hand" file="SYI.hand.mel.xml"/>
  <mel name="wordnet" file="SYI.wordnet.mel.xml"/>

  <reconstruction name="standard">
    <proto_language name="proto" correspondences="SYI.correspondences.xml"/>
    <action name="upstream" target="proto"/>
    <action name="upstream" from="WB,Akha,HaniW,HaniL,Khatu,Pijo,Haoni,Mpi" to="proto"/>
  </reconstruction>
</params>
